{
  "name": "fstack-flags-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -c auto --names tailwind,vite \"npm run tw:dev\" vite",
    "build": "tsc -b && vite build",
